;
; Default BIOS for Titanium Machines
; Does nothing besides write to the screen and boot from Disk
;

; Init display
mov cx 0x4c

mov bx 'T'
mov &0xB800 bx
mov &0xB801 cx

mov bx 'B'
mov &0xB802 bx
mov &0xB803 cx

mov bx 'I'
mov &0xB804 bx
mov &0xB805 cx

mov bx 'O'
mov &0xB806 bx
mov &0xB807 cx

mov bx 'S'
mov &0xB808 bx
mov &0xB809 cx